gg.editAll("0", gg.TYPE_BYTE)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber(":AnoSDKFree", gg.TYPE_BYTE)
gg.getResults(100)
gg.editAll("0", gg.TYPE_BYTE)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber(":AnoSDKGetReportData3", gg.TYPE_BYTE)
gg.getResults(100)
gg.editAll("0", gg.TYPE_BYTE)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber(":AnoSDKGetReportData4", gg.TYPE_BYTE)
gg.getResults(100)
gg.editAll("0", gg.TYPE_BYTE)
